import math 
class Solution:
    def sortedSquares(self, nums: List[int]) -> List[int]:
        k=0
        l=0
        arr=[0]*len(nums)
        p=len(nums)-1
        r=len(nums)-1
        for i in nums:
            if nums[l]**2>=nums[r]**2:
                arr[p]=nums[l]**2
                p-=1
                l+=1
            elif nums[r]**2>nums[l]**2:
                arr[p]=nums[r]**2
                r-=1 
                p-=1  
        return arr
