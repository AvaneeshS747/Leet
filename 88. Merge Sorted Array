class Solution:
    def merge(self, nums1: List[int], m: int, nums2: List[int], n: int) -> None:
        arr=[0]*(m)
        ar=[0]*(n)
        for i in range (m):
            nums1[i]=nums1[i]
        for i in range (n):
            nums1[m+i]=nums2[i]
        length = m+n
        i = 0
        while i < length - 1:
            if (nums1[i] > nums1[i + 1]):
                temp = nums1[i]
                nums1[i] = nums1[i + 1]
                nums1[i + 1] = temp
                i = -1
            i +=1
        

    
        
        
        
